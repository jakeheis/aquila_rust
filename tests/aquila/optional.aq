let maybe_three = Optional[int].some(3);

/// -> 3
if maybe_three.is_some() {
    print[int](*maybe_three.unwrap());
} else {
    print[ptr byte]("Is not any");
}

let maybe_four = Optional[int].none();

/// -> <none>
if maybe_four.is_some() {
    print[int](*maybe_four.unwrap());
} else {
    print[ptr byte]("<none>");
}
