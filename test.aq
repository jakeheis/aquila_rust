type MyObject {
    let num: int;
}

impl MyObject: Freeable {
    def free() {
        print("being freed");
    }
}

def make_box() {
    print("start make box");
    let box = Box[MyObject].new(MyObject(5));
    print("end make box");
}

// print((*box.as_ptr()).num);

print("pre make box");
make_box();
print("post make box");

